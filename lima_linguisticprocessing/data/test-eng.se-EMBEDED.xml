    <test id="eng.se.pers.embed.11.2" trace=".se.xml"
          comment="personne connue"
          left="XPATH#//entities/entity[pos=1]/components/family[@pos=1]"
          operator="="
          right="daughter"/>
    <test id="eng.se.pers.embed.11.3" trace=".se.xml"
          comment="personne connue"
