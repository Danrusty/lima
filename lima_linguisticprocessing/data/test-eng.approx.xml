<?xml version='1.0' encoding='UTF-8'?>
<testcases>
  <!--==================== tests Total.Well ====================-->
  <testcase id="eng.approx.1" type="bloquant">
    <call-parameters>
      <param key="text" value="B"/>
      <param key="language" value="eng"/>
      <list key="pipelines">
        <item value="totalcruncher"/>
      </list>
    </call-parameters>
    <test id="eng.approx.1.1" trace=".se.approx.xml"
          comment="well name of length 1 at start of text ('B' is in the dictionary totalWell-eng.dat)"
          left="XPATH#//specific_entities/specific_entity[position=0][length=1]/type"
          operator="contains"
          right="Total.WELL"/>
    <test id="eng.approx.1.2" trace=".se.approx.xml"
          comment="value of well name is 'B' ('B' is in the dictionary totalWell-eng.dat)"
          left="XPATH#//specific_entities/specific_entity[position=0][length=1]/string"
          operator="="
          right="B"/>
  </testcase>
  <testcase id="eng.approx.2" type="bloquant">
    <call-parameters>
      <param key="text" value="xy yuyu B"/>
      <param key="language" value="eng"/>
      <list key="pipelines">
        <item value="totalcruncher"/>
      </list>
    </call-parameters>
    <test id="eng.approx.2.1" trace=".se.approx.xml"
          comment="name of length 1 somewhere in the text ('B' is in the dictionary totalWell-eng.dat)"
          left="XPATH#//specific_entities/specific_entity[position=8][length=1]/type"
          operator="contains"
          right="Total.WELL"/>
    <test id="eng.approx.2.2" trace=".se.approx.xml"
          comment="value of well name is 'B' ('B' is in the dictionary totalWell-eng.dat)"
          left="XPATH#//specific_entities/specific_entity[position=8][length=1]/string"
          operator="="
          right="B"/>
  </testcase>
  <testcase id="eng.approx.3" type="bloquant">
    <call-parameters>
      <param key="text" value="xy yTrombon 1 hop"/>
      <param key="language" value="eng"/>
      <list key="pipelines">
        <item value="totalcruncher"/>
      </list>
    </call-parameters>
    <test id="eng.approx.3.1" trace=".se.approx.xml"
          comment="name of length 6 with 1 error = 1 inserted character ('Trombon 1' is in the dictionary totalWell-eng.dat)"
          left="XPATH#//specific_entities/specific_entity[position=4][length=9]/type"
          operator="contains"
          right="Total.WELL"/>
  </testcase>
    <test id="eng.approx.3.2" trace=".se.approx.xml"
          comment="value of well name is 'B' ('B' is in the dictionary totalWell-eng.dat)"
          left="XPATH#//specific_entities/specific_entity[position=4][length=9]/string"
          operator="="
          right="Trombon 1"/>
</testcases>
