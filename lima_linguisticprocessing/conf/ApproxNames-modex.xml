  <module name="Processors">
    <group name="ApproxNamesModex" class="ProcessUnitPipeline" >
      <list name="processUnitSequence">
        <item value="wellApproxNamesMatcher"/>
        <item value="blockApproxNamesMatcher"/>
        <item value="fieldApproxNamesMatcher"/>
        <item value="companyApproxNamesMatcher"/>
        <item value="contractApproxNamesMatcher"/>
        <item value="countryApproxNamesMatcher"/>
      </list>
    </group>
    <group name="wellApproxNamesMatcher" class="ApproxStringMatcher">
        <!--param key="dictionary" value="totalWellForms"/-->
        <param key="nameindex" value="well-countryNames"/>
        <param key="nbMaxNumError" value="20"/>
        <param key="nbMaxDenError" value="100"/>
        <param key="entityGoup" value="Total"/>
        <param key="entityName" value="Total.WELL"/>
        <param key="NPCategory" value="NP"/>
        <param key="NPMicroCategory" value="NNP"/>
        <map name="generalization">
            <!-- Delete unbalanced parenthesis -->       
            <!--entry key="^[^\(]*\)|\([^\)]*$" value=""/-->
            <!-- Delete parentheses and content -->       
            <!--entry key="\(.*\)" value=""/-->
            <!-- Delete tailing star (company alias) -->       
            <!--entry key="\\\*$" value=""/-->
            <!-- Group blanks & dashes : A-6 => A( |-)?6 ~= A 6 -->
            <!--entry key="[ |-|#]" value="( |-)?"/-->
            <!-- Add 0 prefix to numerals : A-6 => A-0*6 ~= A-006 -->
            <entry key="([0-9]+)" value="0*\1"/>
        </map>
    </group>
